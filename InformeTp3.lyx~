\begin_layout Standard
\begin_inset Formula 
\[
Y_{1}=w\bar{y_{2}}\bar{y_{1}}+\bar{w}y_{2}\bar{y_{1}}=\bar{y_{1}}(w\bar{y_{2}}+\bar{w}y_{2})=\bar{y_{1}}(y_{2}\oplus w)
\]

\end_inset


